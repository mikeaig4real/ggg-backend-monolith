{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
    "^@modules(|/.*)$": "<rootDir>/src/modules/$1",
    "^@guards(|/.*)$": "<rootDir>/src/guards/$1"
  }
}
